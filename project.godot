; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GodotTest"
run/main_scene="res://Level_1.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Global="*res://global.gd"
SimpleGrass="*res://addons/simplegrasstextured/singleton.tscn"
ItemData="*res://ItemData.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1024
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="EQoT_1"

[editor_plugins]

enabled=PackedStringArray("res://addons/boujie_water_shader/plugin.cfg", "res://addons/simplegrasstextured/plugin.cfg")

[filesystem]

import/fbx/enabled=false

[gui]

theme/default_font_antialiasing=2
theme/default_font_hinting=2
theme/default_font_multichannel_signed_distance_field=true

[importer_defaults]

font_data_dynamic={
"antialiasing": 2,
"fallbacks": [Resource("res://GUI_menus/UI_Elements/fonts/Questrial-Regular.ttf"), Resource("res://GUI_menus/UI_Elements/fonts/Optima.ttc")],
"force_autohinter": true,
"generate_mipmaps": true,
"hinting": 2,
"msdf_pixel_range": 10,
"msdf_size": 64,
"multichannel_signed_distance_field": true
}

